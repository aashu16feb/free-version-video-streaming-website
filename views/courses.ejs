                <% videos.forEach(function (video) { %>
                    <div class="col-xl-3 col-sm-6 mb-3">
                        <div class="video-card">
                            <div class="video-card-image">
                                <a class="play-icon" href="watch?v=<%= video.watch %>"><i
                                        class="fas fa-play-circle"></i></a>
                                <a href="../watch?v=63e3ce755fbdf92bac8538f4">
                                    <img class="thumbnail" src="../public/thumbnails/1674492999418-en2.JPG"
                                        style="width: 270px; height: 169px;">
                                </a>
                                <div class="time">
                                    <%= video.minutes + ":" + video.seconds %>
                                </div>
                            </div>
                            <div class="video-card-body">
                                <div class="video-title">
                                    <a href="watch?v=<%= video.watch %>">
                                        <%= video.title %>
                                    </a>
                                </div>
                                <div class="video-page text-success">

                                    <%= video.category %> <i class="fas fa-check-circle text-success"></i>

                                </div>
                                <div class="video-view">
                                    <%= video.views %> views &nbsp;<i class="fas fa-calendar-alt"></i>
                                        <% var createdAt=new Date(video.createdAt); var date=createdAt.getDate() + "" ;
                                            %>
                                            
                                </div>
                            </div>
                        </div>
                    </div>
                    <% }) %>

